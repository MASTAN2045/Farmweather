{
  "version": 2,
  "builds": [
    {
      "src": "weather-website/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "weather-website/$1"
    }
  ]
} 